.page 
    padding-bottom 2rem
    padding-top 3.5rem

h2 > a
  font-weight inherit

a
  word-break break-word

tr:nth-child(2n)
  background-color #ffffff00

th, td
  border 1pt solid #00000022
  padding 0.5em 0.5em

th
  background-color #0071ba
  color #fdfdfd

a.header-anchor
  font-size .625em
  float left
  margin-top 0.25em
  margin-left -1.4em
  padding-right 0em
  padding-bottom 0.25em
  opacity 0

.theme-default-content:not(.custom) a:hover
  text-decoration none
  border-bottom #0071ba solid 0.0625em

.theme-default-content code
  color #35536f

.line-number, code, kbd
  font-family "source-code-pro", "Menlo", "Monaco", "Consolas", "Courier New", monospace

.custom-block
  &.tip, &.warning, &.danger
    padding 0.125rem 1.5rem
    border-left-width 0.25rem
    border-left-style solid
    margin 1rem 0
    border-radius 20px 0 0 0
    .custom-block-title
      font-size 110%
  &.warning 
    background-color #ffe5644d
    border-color #e7c000
    color #423700
    .custom-block-title
      color #635200
  &.tip
    background-color #e9f6ff
    border-color #0071ba
    color #00356b
    .custom-block-title
      color #0051a2
  &.danger
    background-color #ffe6e6
    border-color #c00
    color #690000
    .custom-block-title
      color #900

.query
  border 1px solid #0071ba
  border-radius 30px
  padding 0.125rem .5rem
  background-color #0071ba0f
  font-size 87.5%
  color #000000df

.examples
  margin 2rem 0 2rem 0
.example
  display flex
  flex-direction column
  justify-content center
  border-bottom 2px dotted #0071bab0
  padding 1rem
.results
  display flex
  justify-content center
  align-self center
  flex-wrap wrap
  margin 1rem 0
.result
  display flex
  flex 1 1 250px
  max-width 500px
  flex-direction column
  padding 1em 1em 2em 1em
  margin 0.1875em
  box-shadow 1px 1px 3px 0px #00000040
  p
    margin 0.5rem 0

.example:last-child
  border-bottom none

@media (min-width: 1280px)
  .results
    width 840px

.note
  font-size 1rem
  color #444
  border-left 0.1875rem solid #aaa
  margin 1rem 0
  padding 0 0 0 1rem
  background -webkit-linear-gradient(left, #f4f4f4, #fff)
  background linear-gradient(90deg, #f4f4f4, #fff)

blockquote
  font-family Roboto,HelveticaNeue,Arial,sans-serif
  font-size 14px
  line-height 1.7
  color #3C4043
  border-left 0.1875rem solid
  border-image linear-gradient(#ea4335, #ea4335 25%, #fbbc05 25%, #fbbc05 50%, #34a853 50%, #34a853 75%, #4285f4 75%) 1
  border-image-width 0 0 0 0.1875rem
  background-color linear-gradient(#ea4335, #ea4335 25%, #fbbc05 25%, #fbbc05 50%, #34a853 50%, #34a853 75%, #4285f4 75%)

dt
  margin-top 0.5rem

dd
  margin-inline-start 2rem

.page-edit .last-updated .time
  color #767676 !important

.page-edit .last-updated .prefix
  color #3f5a75 !important

.page-edit .edit-link a
  color #3f5a75 !important

abbr[title]
  text-decoration none
  cursor help

.source
  font-size 75%
  color #999